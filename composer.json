{
    "name": "rich-id/magic-model-bundle",
    "description": "A magic bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "RichId",
            "homepage": "https://www.rich-id.com"
        }
    ],
    "support": {
        "email": "contact@rich-id.com",
        "source": "https://github.com/rich-id/magic-model-bundle",
        "docs": "https://github.com/rich-id/magic-model-bundle/blob/master/README.md",
        "issues": "https://github.com/rich-id/magic-model-bundle/issues"
    },
    "require": {
        "php": ">=7.3",
        "richcongress/bundle-toolbox": "*"
    },
    "require-dev": {
        "richcongress/test-suite": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "RichId\\MagicModelBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichId\\MagicModelBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
